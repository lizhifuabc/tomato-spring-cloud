<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tomato.merchant.database.MerchantInfoMapper">
    <insert id="insert" parameterType="merchantInfoDO">
        insert into merchant_info(merchant_no,mail, merchant_name, merchant_type)
        values(#{merchantNo},#{mail},#{merchantName},#{merchantType})
    </insert>
</mapper>